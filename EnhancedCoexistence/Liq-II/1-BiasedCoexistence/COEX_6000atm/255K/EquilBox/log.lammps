variable     nsteps_anneal_down_upto	equal (${nsteps_melt_upto}+${nsteps_anneal_down})
variable     nsteps_anneal_down_upto	equal (2200000+${nsteps_anneal_down})
variable     nsteps_anneal_down_upto	equal (2200000+2000000)

variable     nsteps_equil_liquid_upto   equal (${nsteps_anneal_down_upto}+${nsteps_equil_liquid})
variable     nsteps_equil_liquid_upto   equal (4200000+${nsteps_equil_liquid})
variable     nsteps_equil_liquid_upto   equal (4200000+600000)

variable     nsteps_sample_liquid_upto   equal (${nsteps_equil_liquid_upto}+${nsteps_equil_ice})
variable     nsteps_sample_liquid_upto   equal (4800000+${nsteps_equil_ice})
variable     nsteps_sample_liquid_upto   equal (4800000+1000000)

variable     nsteps_coex_upto           equal (${nsteps_sample_liquid_upto}+${nsteps_coex})
